[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .eggs,
    *.egg,
    build,
    dist,
    .tox,
    .pytest_cache,
    .mypy_cache,
    .coverage,
    htmlcov,
    docs/_build
per-file-ignores =
    __init__.py:F401
max-complexity = 10
